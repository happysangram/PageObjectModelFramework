	public HomePage goToSignIn()
	{
		driver.findElement(By.xpath(or.getProperty("userName"))).sendKeys("sangram.das@accruent.com");
		driver.findElement(By.xpath(or.getProperty("password"))).sendKeys("Cooper.123");
		driver.findElement(By.xpath(or.getProperty("signInBtn"))).click();
		return new HomePage();
		
	}

}
