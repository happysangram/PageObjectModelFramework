package com.accruent.pages;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;

public class LoginPage {
	
	WebDriver driver;
	
	public void goToSignIn(String userId, String passkey)
	{
		WebElement email= driver.findElement(By.xpath("//input[@id=\"userID_input\"]"));
		email.sendKeys("sangram.das@accruent.com");
		
		WebElement password= driver.findElement(By.xpath("//input[@id=\"password_input\"]"));
		password.sendKeys("sangram.123");
		
	}

}
